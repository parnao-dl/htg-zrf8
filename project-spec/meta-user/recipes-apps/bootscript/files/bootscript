#!/bin/sh

#-----------------------------------------------------------------------
# Configure HTG ZRF8 Reference Clocks
#


#-----------------------------------------------------------------------
# Variables
#-----------------------------------------------------------------------
# common
vScriptName=`basename $0` # Test script name
vPauseOnError=1
vInteractive=0
vRetVal=0
vTotalChecks=0

# custom
vLastRdVal=0
vMode="cfg"

# Si5341 is normally @ bus=6, address=0x74
vSi5341Found=0
vSi5341Bus=99
vSi5341Addr=74
vSi5341_clk=161

# SCI18IS602 is normally @ bus=14, address=0x2f
vSCI18IS602Found=0
vSCI18IS602Bus=99
vSCI18IS602Addr=99
vSCI18IS602Addr_U73=2e
vSCI18IS602Addr_U72=2a

# LMK04832_U78 is accessed through the SCI18IS602 I2C-to-SPI bridge U73 using slave select vLMKAddr_U78
vLMKAddr_U78=0x01
vLMKRegs0=(0 2 3 4 5 6 12 13)
vLMKRegs1=(115 119 130 131 132 133 136)

# LMX2594_Uxx is accessed through the SCI18IS602 I2C-to-SPI bridge U72 using slave selects vLMXAddr_Uxx
vLMXAddr_U81=0x01
vLMXAddr_U82=0x02
vLMXAddr_U83=0x04

#-----------------------------------------------------------------------
# Functions
#-----------------------------------------------------------------------

# Error monitor
fErrorMon () {
   vTotalChecks=$(( $vTotalChecks + 1 ))
   if [ $1 -gt 0 ]; then
      vRetVal=$(( $vRetVal + 1 ))

      # Pause to review error
      if [ $vPauseOnError -eq 1 ]; then
         echo "Error detected ($vRetVal Errors, $vTotalChecks Checks)!  Hit ENTER to resume..."
         read vDummy
      else
         echo "Error detected ($vRetVal Errors, $vTotalChecks Checks)!"
      fi
   fi
}

# Start banner
fStartBanner () {
   echo ""
   printf "#-------------------Configure HTG ZRF8 Reference Clocks-------------------------\n"
   if [ $vInteractive -eq 1 ]; then
      printf "# $vScriptName: Hit ENTER to start...\n"
      read
   else
      printf "# $vScriptName: STARTED\n"
   fi
   printf "#-------------------------------------------------------------------------------\n"
}

# End banner
fEndBanner () {
   printf "#-------------------------------------------------------------------------------\n"
   if [ $vRetVal -gt 0 ]; then
      printf "# $vScriptName: FINISHED - FAILED [%d Errors, %d Checks]\n" $vRetVal $vTotalChecks
   else
      printf "# $vScriptName: FINISHED - PASSED [%d Checks]\n" $vTotalChecks
   fi
   printf "#-------------------------------------------------------------------------------\n"
   echo ""
   exit $vRetVal
}


# I2C Paged Read with expected value and masking support
fI2cRdPaged () {
   # 1 - bus
   # 2 - device address
   # 3 - page register address
   # 4 - page number
   # 5 - register address
   # 6 - expected read value
   # 7 - read value checking mask
   vExp=$(( 0 + $6 ))
   vMask=$(( 0 + $7 ))

   # Set the page offset
   i2cset -y $1 $2 $3 $4; fErrorMon $?
   # i2cget -y $1 $2 $5
   vRdVal=`i2cget -y $1 $2 $5 | awk '{ print $1 }'`; fErrorMon $?
   vLastRdVal=$vRdVal
   if [ $vMask -eq 0 ]; then
      printf "i2c_rdp $1 $2 ${4}_${5} $vRdVal [Exp=0x--] - No Check\n"
   else
      vRdValMask=$(( $vRdVal & $vMask ))
      vExpMask=$(( $vExp & $vMask ))
      if [ $vRdValMask -eq $vExpMask ]; then
         printf "i2c_rdp $1 $2 ${4}_${5} $vRdVal [Mask=0x%02x, MaskedVal=0x%02x] - Match\n" $vMask $vRdValMask
      else
         printf "i2c_rdp $1 $2 ${4}_${5} $vRdVal [Mask=0x%02x, MaskedVal=0x%02x, Exp=0x%02x, ExpMaskedVal=0x%02x] - Mismatch Error!\n" $vMask $vRdValMask $vExp $vExpMask
         fErrorMon 1
      fi
   fi
}


# List devices on all I2C buses
fListBusDevices () {
   for (( ii=0; ii<30; ii++ )); do
      ls -l /dev/i2c* | grep i2c-${ii} > /dev/null
      if [ $? -eq 0 ]; then
         echo "i2c-${ii} bus:"
         i2cdetect -y -r ${ii}
         echo ""
      fi
   done
}


# Find Si5341
fFindSi5341 () {
   printf "Scanning for Si5341... "
   for (( ii=0; ii<30; ii++ )); do
      #ls -l /dev/i2c* | grep i2c-${ii} > /dev/null
      #if [ $? -eq 0 ]; then
      if [ -e /dev/i2c-${ii} ]; then
         # Check for Si5341
         #echo "i2c-${ii} bus:"
         #i2cdetect -y -r ${ii} | grep -i $vSi5341Addr > /dev/null
	 ls /sys/bus/i2c/devices | grep -i ${ii}-00$vSi5341Addr> /dev/null
         if [ $? -eq 0 ]; then
            vSi5341Bus=${ii}
            vSi5341Addr="0x${vSi5341Addr}"
            echo "Detected Si5341 at bus=${vSi5341Bus}, address=${vSi5341Addr}."
            vSi5341Found=1
         fi
      fi
   done

   # Temporary hack to avoid invalid detection of device on bus 1
   if [ $vSi5341Bus -eq 1 ]; then
      echo "Warning - Si5341 unexpectedly detected on bus 1 - rescanning..."

      # Restore variable default
      vSi5341Found=0
      vSi5341Bus=99
      vSi5341Addr=68

      for (( ii=0; ii<30; ii++ )); do
         #ls -l /dev/i2c* | grep i2c-${ii} > /dev/null
         #if [ $? -eq 0 ]; then
         if [ -e /dev/i2c-${ii} ]; then
            # Check for Si5341
            #echo "i2c-${ii} bus:"
            i2cdetect -y -r ${ii} | grep -i $vSi5341Addr > /dev/null
            if [ $? -eq 0 ]; then
               vSi5341Bus=${ii}
               vSi5341Addr="0x${vSi5341Addr}"
               echo "Detected Si5341 at bus=${vSi5341Bus}, address=${vSi5341Addr}."
               vSi5341Found=1
            fi
         fi
      done
   fi

   if [ $vSi5341Found -eq 0 ]; then
      echo "Warning: Si5341 device not found!"
   fi
}


# Find SCI18IS602
fFindSCI18IS602 () {
   printf "Scanning for SCI18IS602... "
   for (( ii=0; ii<30; ii++ )); do
      #ls -l /dev/i2c* | grep i2c-${ii} > /dev/null
      #if [ $? -eq 0 ]; then
      if [ -e /dev/i2c-${ii} ]; then
         # Check for SCI18IS602
         #echo "i2c-${ii} bus:"
         #i2cdetect -y -r ${ii} | grep -i $vSCI18IS602Addr > /dev/null
	 ls /sys/bus/i2c/devices | grep -i ${ii}-00$vSCI18IS602Addr > /dev/null
         if [ $? -eq 0 ]; then
            vSCI18IS602Bus=${ii}
            vSCI18IS602Addr="0x${vSCI18IS602Addr}"
            echo "Detected SCI18IS602 at bus=${vSCI18IS602Bus}, address=${vSCI18IS602Addr}."
            vSCI18IS602Found=1
         fi
      fi
   done
   if [ $vSCI18IS602Found -eq 0 ]; then
      echo "Warning: SCI18IS602 device not found!"
   fi
}

# Configure LMK04832
fCfgLMK04832 () {
	# Reference: https://manpages.debian.org/unstable/i2c-tools/i2cset.8.en.html
	# Scan bus to find SCI18IS602 I2C-to-SPI mux (assume LMK04832 is on this bus)
   	vSCI18IS602Addr=${vSCI18IS602Addr_U73}
	fFindSCI18IS602

	if [ $vSCI18IS602Found -eq 1 ]; then

	# Configure SCI18IS602 operating mode
	i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr 0xf0 0x02 i; fErrorMon $?
	#i2cget -f -y $vSCI18IS602Bus $vSCI18IS602Addr 0xf0; fErrorMon $?

	# enable LMK04832 spi readback (pin 31)
       	i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x5f 0x3b i; fErrorMon $?

        # Load LMK registers (source LMK04832_ckin0=12M8_ck0_7m68_ck1_7m68_ck2_122m88_ck4_122m88.txt)
        # R0 (INIT) 0x00160040
        # R0        0x20143000
        # R1        0x00143001
        # R2        0x00140302
        # R3        0xC0140023
        # R4        0x40140024
        # R5        0x80141E05
        # R6        0x01100006
        # R7        0x01100007
        # R8        0x06010008
        # R9        0x55555549
        # R10       0x9102410A
        # R11       0x0401100B
        # R12       0x1B0C006C
        # R13       0x2302806D
        # R14       0x0200000E
        # R15       0x8000800F
        # R16       0xC1550410
        # R24       0x00000058
        # R25       0x02C9C419
        # R26       0x8FA8001A
        # R27       0x10001E1B
        # R28       0x0021201C
        # R29       0x0180031D
        # R30       0x0200031E
        # R31       0x003F001F

        printf "Configuring LMK04832_U78\n"
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x00 0x00 0x90 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x00 0x00 0x10 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x00 0x02 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x00 0x03 0x06 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x00 0x04 0xD1 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x00 0x05 0x63 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x00 0x06 0x50 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x00 0x0C 0x51 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x00 0x0D 0x04 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x00 0x0C i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x01 0x0A i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x02 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x03 0x40 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x04 0x10 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x05 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x06 0x01 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x07 0x01 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x08 0x04 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x09 0x0A i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x0A 0x80 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x0B 0x40 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x0C 0x10 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x0D 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x0E 0x01 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x0F 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x10 0x08 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x11 0x0A i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x12 0x80 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x13 0x40 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x14 0x10 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x15 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x16 0x01 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x17 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x18 0x08 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x19 0x0A i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x1A 0x80 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x1B 0x40 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x1C 0x10 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x1D 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x1E 0x01 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x1F 0x30 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x20 0x08 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x21 0x0A i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x22 0x80 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x23 0x40 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x24 0x10 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x25 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x26 0x01 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x27 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x28 0x08 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x29 0x0A i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x2A 0x80 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x2B 0x40 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x2C 0x10 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x2D 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x2E 0x01 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x2F 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x30 0x08 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x31 0x0A i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x32 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x33 0x40 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x34 0x10 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x35 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x36 0x01 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x37 0x10 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x38 0x26 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x39 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x3A 0x0C i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x3B 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x3C 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x3D 0x08 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x3E 0x03 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x3F 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x40 0x0F i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x41 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x42 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x43 0x11 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x44 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x45 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x46 0x08 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x47 0x1A i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x48 0x02 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x49 0x42 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x4A 0x03 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x4B 0x06 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x4C 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x4D 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x4E 0xC0 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x4F 0x7F i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x50 0x01 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x51 0x02 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x52 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x53 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x54 0x78 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x55 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x56 0x78 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x57 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x58 0x96 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x59 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x5A 0x78 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x5B 0xD4 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x5C 0x20 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x5D 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x5E 0x1E i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x5F 0x0B i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x60 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x61 0x01 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x62 0x4C i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x63 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x64 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x65 0x0C i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x69 0x58 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x6A 0x20 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x6B 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x6C 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x6D 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x6E 0x13 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x73 0x10 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x77 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x82 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x83 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x66 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x67 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x01 0x68 0x0C i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMKAddr_U78 0x05 0x55 0x00 i; fErrorMon $?
        printf "Configuring LMK04832_U78... done\n"

        printf "LMK04832_U78 Configuration\n"
	# 0x000, 0x002, 0x003, 0x004, 0x005, 0x006, 0x00C, 0x00D
	for reg in ${vLMKRegs0[@]}; do
        	i2ctransfer -f -y $vSCI18IS602Bus w4@$vSCI18IS602Addr $vLMKAddr_U78 0x80 ${reg} 0x00; fErrorMon $?
        	if [ $? -eq 0 ]; then
			printf "0x%03x:" "${reg}"
		i2ctransfer -f -y $vSCI18IS602Bus r3@$vSCI18IS602Addr | cut -b11,12,13,14; fErrorMon $?
		fi
	done

	# 0x100 - 0x16c
	for (( ii=0, jj=0x100; ii<109; ii++, jj++)); do
		i2ctransfer -f -y $vSCI18IS602Bus w4@$vSCI18IS602Addr $vLMKAddr_U78 0x81 ${ii} 0x00; fErrorMon $?
		if [ $? -eq 0 ]; then
			printf "0x%03x:" "${jj}"
			i2ctransfer -f -y $vSCI18IS602Bus r3@$vSCI18IS602Addr | cut -b11,12,13,14; fErrorMon $?
                fi
	done
	fi

	# 0x173, 0x177, 0x182, 0x183, 0x184, 0x185, 0x188
	for reg in ${vLMKRegs1[@]}; do
        	i2ctransfer -f -y $vSCI18IS602Bus w4@$vSCI18IS602Addr $vLMKAddr_U78 0x81 ${reg} 0x00; fErrorMon $?
        	if [ $? -eq 0 ]; then
			printf "0x1%02x:" "${reg}"
		i2ctransfer -f -y $vSCI18IS602Bus r3@$vSCI18IS602Addr | cut -b11,12,13,14; fErrorMon $?
		fi
	done

	# 0x555
        i2ctransfer -f -y $vSCI18IS602Bus w4@$vSCI18IS602Addr $vLMKAddr_U78 0x85 0x55 0x00; fErrorMon $?
        if [ $? -eq 0 ]; then
		printf "0x555:" "${reg}"
		i2ctransfer -f -y $vSCI18IS602Bus r3@$vSCI18IS602Addr | cut -b11,12,13,14; fErrorMon $?
	fi
}

# Configure LMX2594
fCfgLMX2594 () {
	# Reference: https://manpages.debian.org/unstable/i2c-tools/i2cset.8.en.html
	# Scan bus to find SCI18IS602 I2C-to-SPI mux (assume LMX2594 is on this bus)
   	vSCI18IS602Addr=${vSCI18IS602Addr_U72}
	fFindSCI18IS602

	if [ $vSCI18IS602Found -eq 1 ]; then
	printf "Configuring LMX2594_U81\n"
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x70 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x6F 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x6E 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x6D 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x6C 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x6B 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x6A 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x69 0x00 0x21 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x68 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x67 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x66 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x65 0x00 0x11 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x64 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x63 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x62 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x61 0x08 0x88 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x60 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x5F 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x5E 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x5D 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x5C 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x5B 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x5A 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x59 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x58 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x57 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x56 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x55 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x54 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x53 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x52 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x51 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x50 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x4F 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x4E 0x00 0x03 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x4D 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x4C 0x00 0x0C i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x4B 0x08 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x4A 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x49 0x00 0x3F i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x48 0x00 0x01 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x47 0x00 0x81 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x46 0xC3 0x50 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x45 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x44 0x03 0xE8 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x43 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x42 0x01 0xF4 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x41 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x40 0x13 0x88 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x3F 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x3E 0x03 0x22 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x3D 0x00 0xA8 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x3C 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x3B 0x00 0x01 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x3A 0x90 0x01 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x39 0x00 0x20 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x38 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x37 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x36 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x35 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x34 0x08 0x20 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x33 0x00 0x80 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x32 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x31 0x41 0x80 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x30 0x03 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x2F 0x03 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x2E 0x07 0xFC i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x2D 0xC0 0xDF i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x2C 0x1F 0xA3 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x2B 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x2A 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x29 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x28 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x27 0x03 0xE8 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x26 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x25 0x04 0x04 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x24 0x00 0x40 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x23 0x00 0x04 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x22 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x21 0x1E 0x21 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x20 0x03 0x93 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x1F 0x43 0xEC i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x1E 0x31 0x8C i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x1D 0x31 0x8C i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x1C 0x04 0x88 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x1B 0x00 0x02 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x1A 0x0D 0xB0 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x19 0x0C 0x2B i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x18 0x07 0x1A i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x17 0x00 0x7C i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x16 0x00 0x01 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x15 0x04 0x01 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x14 0xE0 0x48 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x13 0x27 0xB7 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x12 0x00 0x64 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x11 0x01 0x2C i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x10 0x00 0x80 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x0F 0x06 0x4F i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x0E 0x1E 0x70 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x0D 0x40 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x0C 0x50 0x01 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x0B 0x00 0x18 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x0A 0x10 0xD8 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x09 0x16 0x04 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x08 0x20 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x07 0x40 0xB2 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x06 0xC8 0x02 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x05 0x00 0xC8 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x04 0x0A 0x43 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x03 0x06 0x42 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x02 0x05 0x00 i; fErrorMon $?
