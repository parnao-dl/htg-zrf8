        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x47 0x00 0x81 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x46 0xC3 0x50 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x45 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x44 0x03 0xE8 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x43 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x42 0x01 0xF4 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x41 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x40 0x13 0x88 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x3F 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x3E 0x03 0x22 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x3D 0x00 0xA8 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x3C 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x3B 0x00 0x01 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x3A 0x90 0x01 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x39 0x00 0x20 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x38 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x37 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x36 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x35 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x34 0x08 0x20 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x33 0x00 0x80 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x32 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x31 0x41 0x80 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x30 0x03 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x2F 0x03 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x2E 0x07 0xFC i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x2D 0xC0 0xDF i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x2C 0x1F 0xA3 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x2B 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x2A 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x29 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x28 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x27 0x03 0xE8 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x26 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x25 0x04 0x04 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x24 0x00 0x40 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x23 0x00 0x04 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x22 0x00 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x21 0x1E 0x21 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x20 0x03 0x93 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x1F 0x43 0xEC i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x1E 0x31 0x8C i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x1D 0x31 0x8C i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x1C 0x04 0x88 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x1B 0x00 0x02 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x1A 0x0D 0xB0 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x19 0x0C 0x2B i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x18 0x07 0x1A i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x17 0x00 0x7C i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x16 0x00 0x01 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x15 0x04 0x01 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x14 0xE0 0x48 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x13 0x27 0xB7 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x12 0x00 0x64 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x11 0x01 0x2C i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x10 0x00 0x80 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x0F 0x06 0x4F i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x0E 0x1E 0x70 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x0D 0x40 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x0C 0x50 0x01 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x0B 0x00 0x18 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x0A 0x10 0xD8 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x09 0x16 0x04 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x08 0x20 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x07 0x40 0xB2 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x06 0xC8 0x02 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x05 0x00 0xC8 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x04 0x0A 0x43 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x03 0x06 0x42 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x02 0x05 0x00 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x01 0x08 0x08 i; fErrorMon $?
        i2cset -f -y $vSCI18IS602Bus $vSCI18IS602Addr $vLMXAddr_U81 0x00 0x25 0x1C i; fErrorMon $?
	printf "Configuring LMX2594_U83.... done\n"
	fi
}

# Configure Si5341 PLL 
fCfgSi5341 () {
    fFindSi5341

      if [ $vSi5341Found -eq 1 ]; then
         # Use 161 for designs with rev 1.0.x, or for later FHA designs
         if ( [ $vSi5341_clk == "161" ] ); then
            printf "Configuring Si5341 output for 161.132812 MHz operation...\n"
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x0B; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x24 0xC0; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x0B; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x25 0x04; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x05; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x40 0x01; fErrorMon $?
            sleep 1
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x06 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x07 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x08 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x0B 0x68; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x16 0x03; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x17 0xDC; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x18 0xEE; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x19 0xFF; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x1A 0xFF; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x20 0x02; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x2B 0x02; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x2C 0x01; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x2D 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x2E 0x3C; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x2F 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x30 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x31 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x32 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x33 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x34 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x35 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x36 0x02; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x37 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x38 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x39 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x3A 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x3B 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x3C 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x3D 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x3E 0x10; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x3F 0x11; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x40 0x04; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x41 0x0E; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x42 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x43 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x44 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x45 0x0C; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x46 0x32; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x47 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x48 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x49 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x4A 0x32; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x4B 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x4C 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x4D 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x4E 0x05; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x4F 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x50 0x0F; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x51 0x03; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x52 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x53 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x54 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x55 0x03; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x56 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x57 0x00; fErrorMon $?
            i2cset -f -y $vSi5341Bus $vSi5341Addr 0x01 0x00; fErrorMon $?; i2cset -f -y $vSi5341Bus $vSi5341Addr 0x58 0x00; fErrorMon $?
